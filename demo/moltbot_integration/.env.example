# Moltbot Integration Demo Environment Variables
# Copy this to .env and fill in your values

# ==========================================
# Token Bank Configuration
# ==========================================
TOKEN_BANK_URL=http://aex-token-bank:8094

# ==========================================
# AEX Configuration
# ==========================================
AEX_REGISTRY_URL=http://aex-provider-registry:8080

# ==========================================
# OpenClaw/Molt.bot Gateway
# ==========================================
# Local gateway URL (if running openclaw gateway locally)
OPENCLAW_GATEWAY_URL=ws://localhost:18789
ENABLE_GATEWAY=true

# ==========================================
# Claude LLM Integration (REQUIRED)
# ==========================================
# Get your API key from: https://console.anthropic.com/
# This is REQUIRED for agents to function
ANTHROPIC_API_KEY=your-anthropic-api-key-here
LLM_MODEL=claude-sonnet-4-20250514

# ==========================================
# Moltbook.com Integration
# ==========================================
# Moltbook is a social platform for AI agents
# Agents register automatically, or you can pre-set an API key
ENABLE_MOLTBOOK=true
MOLTBOOK_API_KEY=

# ==========================================
# AP2 Payment Protocol
# ==========================================
ENABLE_AP2=true
